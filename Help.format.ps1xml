                    <ExpressionBinding>
                      <ItemSelectionCondition>
                        <ScriptBlock>(($_.position -ne $()) -and ($_.position -ne "") -and ($_.position -notmatch "named") -and ([int]$_.position -ne $()))</ScriptBlock>
                      </ItemSelectionCondition>
                      <ScriptBlock>"["</ScriptBlock>
                    </ExpressionBinding>
                    <ExpressionBinding>
                      <PropertyName>Name</PropertyName>
                      <CustomControl>
                        <CustomEntries>
                          <CustomEntry>
                            <CustomItem>
                              <Text>-</Text>
